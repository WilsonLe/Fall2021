{
	"python.languageServer": "Jedi",
	"python.pythonPath": "./venv/bin/python",
	// "python.linting.pylintEnabled": true,
	// "python.linting.enabled": true,
	// "python.linting.lintOnSave": true,
	// "python.linting.pylintArgs": [
	// 	"--indent-string='\t'",
	// 	"--disable=C0103"
	// ],
	"python.formatting.provider": "autopep8",
	"python.formatting.autopep8Args": [
		"--indent-size=2",	
		"--indent-string='\t'"
	],
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"editor.autoClosingQuotes": "always",
	"editor.tabSize": 2,
	"jupyter.jupyterServerType": "local",
	"files.associations": {
		"*.py": "python",
		"*.js": "javascript",
		"*.csv": "csv",
		"deque": "cpp",
		"list": "cpp",
		"string": "cpp",
		"unordered_map": "cpp",
		"unordered_set": "cpp",
		"vector": "cpp",
		"__hash_table": "cpp",
		"__split_buffer": "cpp",
		"__tree": "cpp",
		"array": "cpp",
		"bitset": "cpp",
		"initializer_list": "cpp",
		"iterator": "cpp",
		"map": "cpp",
		"queue": "cpp",
		"random": "cpp",
		"regex": "cpp",
		"set": "cpp",
		"span": "cpp",
		"stack": "cpp",
		"string_view": "cpp",
		"utility": "cpp",
		"valarray": "cpp",
		"iostream": "cpp",
		"ostream": "cpp"
	}
}